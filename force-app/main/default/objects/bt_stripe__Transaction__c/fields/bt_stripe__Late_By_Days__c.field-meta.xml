<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>bt_stripe__Late_By_Days__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>If (ISPICKVAL(  bt_stripe__Transaction_Status__c  ,&apos;Open&apos;)&amp;&amp;(today()-  bt_stripe__Due_Date__c )&gt;0,today()- bt_stripe__Due_Date__c,NULL)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Formula field, displays the number of days between today&apos;s date and the due date. Only populates when Transaction Status equals &apos;Open&apos;.</inlineHelpText>
    <label>Late By (Days)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
